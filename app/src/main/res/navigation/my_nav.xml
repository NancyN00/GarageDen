<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/homeFragment3">

    <fragment
        android:id="@+id/homeFragment3"
        android:name="com.example.garageden.botmnavfragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment3_to_profileFragment2"
            app:destination="@id/profileFragment2" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment2"
        android:name="com.example.garageden.botmnavfragments.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment2_to_servicesFragment2"
            app:destination="@id/servicesFragment2" />
    </fragment>
    <fragment
        android:id="@+id/servicesFragment2"
        android:name="com.example.garageden.botmnavfragments.ServicesFragment"
        android:label="fragment_services"
        tools:layout="@layout/fragment_services" >
        <action
            android:id="@+id/action_servicesFragment2_to_filltersFragment"
            app:destination="@id/filltersFragment" />
    </fragment>
    <fragment
        android:id="@+id/filltersFragment"
        android:name="com.example.garageden.botmnavfragments.FiltersFragment"
        android:label="fragment_fillers"
        tools:layout="@layout/fragment_fillers" />
</navigation>